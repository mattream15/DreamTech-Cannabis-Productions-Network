<h1>Plant Your Cannabis Seeds</h1>
<form method="POST" action="/cannabis_plants">

    <select name="user_id">
        <% @users.each do |user| %>
            <option value="<%= user.id%>"> <%= user.name %> </option>
        <% end %>
    </select>

<h3>Cannabis Plant ID: <input type="text" name="cannabis_plant_id"></h3>
<input type="submit">
</form>